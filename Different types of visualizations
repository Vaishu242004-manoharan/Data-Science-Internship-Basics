      "source": [
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Load dataset\n",
        "iris = sns.load_dataset(\"iris\")\n",
        "\n",
        "# Prepare data for pie chart\n",
        "species_counts = iris['species'].value_counts()\n",
        "pie_colors = ['#66c2a5', '#fc8d62', '#8da0cb']\n",
        "\n",
        "# Set up the layout: 3 rows, 1 column\n",
        "fig, axes = plt.subplots(3, 1, figsize=(10, 18))\n",
        "\n",
        "# 1️⃣ Bar Chart – Count of Each Iris Species\n",
        "sns.countplot(ax=axes[0], x='species', data=iris, palette='viridis')\n",
        "axes[0].set_title('🌼 Count of Each Iris Species', fontsize=14, fontweight='bold')\n",
        "axes[0].set_xlabel('Species', fontsize=12)\n",
        "axes[0].set_ylabel('Count', fontsize=12)\n",
        "axes[0].grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# 2️⃣ Scatter Plot – Sepal Length vs Sepal Width\n",
        "sns.scatterplot(\n",
        "    ax=axes[1],\n",
        "    x='sepal_length',\n",
        "    y='sepal_width',\n",
        "    hue='species',\n",
        "    style='species',\n",
        "    palette='Set1',\n",
        "    data=iris,\n",
        "    s=100\n",
        ")\n",
        "axes[1].set_title('📈 Sepal Length vs Sepal Width by Species', fontsize=14, fontweight='bold')\n",
        "axes[1].set_xlabel('Sepal Length (cm)', fontsize=12)\n",
        "axes[1].set_ylabel('Sepal Width (cm)', fontsize=12)\n",
        "axes[1].legend(title='Species')\n",
        "axes[1].grid(True, linestyle='--', alpha=0.5)\n",
        "\n",
        "# 3️⃣ Pie Chart – Proportion of Each Iris Species\n",
        "axes[2].pie(\n",
        "    species_counts,\n",
        "    labels=species_counts.index,\n",
        "    autopct='%1.1f%%',\n",
        "    startangle=140,\n",
        "    colors=pie_colors,\n",
        "    wedgeprops={'edgecolor': 'white'}\n",
        ")\n",
        "axes[2].set_title('🥧 Proportion of Each Iris Species', fontsize=14, fontweight='bold')\n",
        "axes[2].axis('equal')  # Keeps pie chart as a circle\n",
        "\n",
        "# Adjust layout\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}